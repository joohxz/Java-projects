package classe;

public class ProdutoTeste {
	public static void main(String[] args) {
		Produto p1 = new Produto();
		p1.nome = "Notebook";
		p1.preco = 2544.99;
		p1.desconto = 0.10;
		
		
		var p2 = new Produto();
		p2.nome = "Caneta preta BIC";
		p2.preco = 1.99;
		p2.desconto = 0;
		
		System.out.println(p1.nome);
		System.out.println(p2.nome);
	}
}
